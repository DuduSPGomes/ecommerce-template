{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@App/*": ["src/*"],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/modules/catalog/data/product.query.ts" ],
  "exclude": [
    "node_modules"
  ]
}
